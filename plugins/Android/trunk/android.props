android.createAVD.label=Create AVD
android.createProject.label=Create Project
android.launchAVD.label=Launch AVD
android.launchDDMS.label=Launch Debug Monitor
android.listTargets.label=List Targets
android.listAVDs.label=List AVDs
android.buildDebug.label=Build App (Debug)
