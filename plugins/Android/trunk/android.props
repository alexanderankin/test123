# Properties for Android Plugin
#-----------------------------------

plugin.android.AndroidPlugin.name=Android
plugin.android.AndroidPlugin.author=Dale Anson
plugin.android.AndroidPlugin.version=1.1
plugin.android.AndroidPlugin.activate=defer
plugin.android.AndroidPlugin.description=Android plugin provides some utilities to help create and run Android projects.

# dependencies
plugin.android.AndroidPlugin.depend.0=jdk 1.6
plugin.android.AndroidPlugin.depend.1=jedit 05.03.00.00
plugin.android.AndroidPlugin.depend.2=plugin CommonControlsPlugin 1.7.4
plugin.android.AndroidPlugin.depend.3=plugin projectviewer.ProjectPlugin 3.5.1
plugin.android.AndroidPlugin.depend.4=plugin console.ConsolePlugin 5.1.4

# docs
plugin.android.AndroidPlugin.docs=index.html

# menu settings
plugin.android.AndroidPlugin.menu.label=Android
plugin.android.AndroidPlugin.menu=\
    android.createAVD \
    android.createProject \
    android.launchAVD \
    android.launchDDMS \
    - \
    android.buildDebug \
    android.deployDebug \
    - \
    android.listTargets \
    android.listAVDs

