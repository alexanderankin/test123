<?xml version="1.0"?>

<!DOCTYPE ACTIONS SYSTEM "actions.dtd">

<ACTIONS>
	<ACTION NAME="actionhooks-enabled" NO_REPEAT="TRUE">
		<CODE>
			actionhooks.ActionHooksPlugin.setEnabled(
				!actionhooks.ActionHooksPlugin.getEnabled());
		</CODE>
		<IS_SELECTED>
			return actionhooks.ActionHooksPlugin.getEnabled();
		</IS_SELECTED>
	</ACTION>
</ACTIONS>

