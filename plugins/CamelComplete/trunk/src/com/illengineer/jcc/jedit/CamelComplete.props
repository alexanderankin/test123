# jEdit only needs to load the plugin the first time the user accesses it
# the presence of this property also tells jEdit the plugin is using the new API
plugin.com.illengineer.jcc.jedit.CamelCompletePlugin.activate=defer
plugin.com.illengineer.jcc.jedit.CamelCompletePlugin.usePluginHome=true

# These two properties are required for all plugins
plugin.com.illengineer.jcc.jedit.CamelCompletePlugin.name=CamelComplete
plugin.com.illengineer.jcc.jedit.CamelCompletePlugin.author=Jesse Pavel

# version number
plugin.com.illengineer.jcc.jedit.CamelCompletePlugin.version=2.3

# online help
plugin.com.illengineer.jcc.jedit.CamelCompletePlugin.docs=CamelComplete.html

# we only have one dependency, jEdit 4.3
plugin.com.illengineer.jcc.jedit.CamelCompletePlugin.depend.0=jedit 04.03.10.00
plugin.com.illengineer.jcc.jedit.CamelCompletePlugin.depend.1=jdk 1.5

plugin.com.illengineer.jcc.jedit.CamelCompletePlugin.description=This plugin provides for \
		code-completion in a CamelCase or regex_separated_identifier style.

# plugin menu
plugin.com.illengineer.jcc.jedit.CamelCompletePlugin.menu=camelcomplete.complete \
    camelcomplete.process_engine camelcomplete.process_all_engines camelcomplete.clear_cache

# action labels for actions supplied by actions.xml
camelcomplete.complete.label=CamelComplete Word
camelcomplete.process_engine.label=Update an Engine...
camelcomplete.process_all_engines.label=Update All Engines
camelcomplete.clear_cache.label=Clear Cache Dir

# plugin option pane
plugin.com.illengineer.jcc.jedit.CamelCompletePlugin.option-pane=camelcomplete
options.camelcomplete.label=CamelComplete
options.camelcomplete.code=new com.illengineer.jcc.jedit.OptionPanel();
