	<ACTION NAME="debugger-toggle-breakpoint" NO_REPEAT="TRUE">
		<CODE>
			debugger.jedit.Plugin.toggleBreakpoint(view);
		</CODE>
	</ACTION>
	<ACTION NAME="debugger-set-breakpoint" NO_REPEAT="TRUE">
		<CODE>
			debugger.jedit.Plugin.setBreakpoint(view);
