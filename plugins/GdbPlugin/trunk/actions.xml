<ACTIONS>
	
	<ACTION NAME="debugger-quit" NO_REPEAT="TRUE">
		<CODE>
			debugger.jedit.Plugin.quit(view);
		</CODE>
	</ACTION>
	<ACTION NAME="debugger-go" NO_REPEAT="TRUE">
		<CODE>
			debugger.jedit.Plugin.go(view);
		</CODE>
	</ACTION>
	<ACTION NAME="debugger-next" NO_REPEAT="TRUE">
