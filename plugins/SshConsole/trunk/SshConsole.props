# Plugin properties
plugin.console.ssh.SshConsolePlugin.activate=defer
plugin.console.ssh.SshConsolePlugin.name=SshConsole
plugin.console.ssh.SshConsolePlugin.author=Alan Ezust
plugin.console.ssh.SshConsolePlugin.version=1.0.3
plugin.console.ssh.SshConsolePlugin.docs=index.html
plugin.console.ssh.SshConsolePlugin.description=Allows you to issue compile commands remotely via ssh in the same location as the sftp:// file you are editing, and parses the output for errors in ErrorList.
plugin.console.ssh.SshConsolePlugin.depend.0=jdk 1.6
plugin.console.ssh.SshConsolePlugin.depend.1=jedit 05.00.01.00
plugin.console.ssh.SshConsolePlugin.depend.2=plugin errorlist.ErrorListPlugin 2.0
plugin.console.ssh.SshConsolePlugin.depend.3=plugin ftp.FtpPlugin 1.0.0
plugin.console.ssh.SshConsolePlugin.depend.4=plugin console.ConsolePlugin 5.0

plugin.console.ssh.SshConsolePlugin.menu=open-sftp console.shell.ssh-show console.ssh.close-connections
plugin.console.ssh.SshConsolePlugin.usePluginHome=true
console.ssh.close-connections.label=Close all connections
console.shell.ssh-show.label=Show SshConsole
