#{{{ general plugin properites
plugin.csidekick.Plugin.activate=defer
plugin.csidekick.Plugin.name=CSideKick
plugin.csidekick.Plugin.author=Alan Ezust
plugin.csidekick.Plugin.version=0.1
plugin.csidekick.Plugin.docs=index.html
# }}}
# {{{ Project option pane properties
plugin.projectviewer.csidekick.Plugin.option-pane=csidekick
options.csidekick.label=C/C++
options.csidekick.code=new csidekick.CSideKickOptionPane();
options.csidekick.includepath.label=INCLUDEPATH
options.csidekick.useinproject.label=Use C/C++ SideKick in this Project
# }}}
#{{{ dependencies
plugin.csidekick.Plugin.depend.0=jdk 1.4.2
plugin.csidekick.Plugin.depend.1=jedit 04.02.99.00
plugin.csidekick.Plugin.depend.2=plugin errorlist.ErrorListPlugin 1.4
plugin.csidekick.Plugin.depend.3=plugin sidekick.SideKickPlugin 0.6
plugin.csidekick.Plugin.depend.4=optional plugin projectviewer.ProjectPlugin 2.2.0

# }}}

#{{{ menu
plugin.csidekick.Plugin.menu=\
	structure-browser
# }}}

#{{{ registration with SideKick as a mode parser
sidekick.parser.c++.label=c++
mode.c++.sidekick.parser=c++
sidekick.parser.c.label=c
mode.c.sidekick.parser=c

# }}}

# {{{ Options Labels
options.sidekick.c.useinproject=use CSideKick in project {0}
# }}}
