
<!DOCTYPE ACTIONS SYSTEM "actions.dtd">
<!--
   Qt4jEdit actions
-->
<ACTIONS>
   <ACTION NAME="assistant-start">
     <CODE>
	 	qt4jedit.Qt4jEditPlugin.instance().startAssistant();
	 </CODE>
   </ACTION>
   <ACTION NAME="assistant-stop">
   	<CODE>
	 	qt4jedit.Qt4jEditPlugin.instance().stopAssistant();
	</CODE>
   </ACTION>
   <ACTION NAME="assistant-activate-identifier">
     <CODE>
	    qt4jedit.Qt4jEditPlugin.instance().activateIdentifier(textArea);
	 </CODE>
   </ACTION>
   <ACTION NAME="assistant-activate-keyword">
     <CODE>
	    qt4jedit.Qt4jEditPlugin.instance().activateKeyword(textArea);
	 </CODE>
   </ACTION>
   <ACTION NAME="assistant-set-url">
     <CODE>
	    qt4jedit.Qt4jEditPlugin.instance().openURL(textArea);
	 </CODE>
   </ACTION>
</ACTIONS>

