#the presence of this property also tells jEdit the plugin is using the new API
##plugin.h4JmfPlugin.activate=defer
plugin.h4JmfPlugin.activate=startup

# These two properties are required for all plugins
plugin.h4JmfPlugin.name=h4Jmf
plugin.h4JmfPlugin.author=Herman Vierendeels

# version number == jEdit version number
plugin.h4JmfPlugin.version=0.0.1


# we only have one dependency, jEdit 4.2, since we use the new plugin API
plugin.h4JmfPlugin.depend.0=jedit 04.03.15.00
plugin.h4JmfPlugin.depend.1=jdk 1.5

plugin.h4JmfPlugin.description=test with Java Media Framework, control audio-player from jEdit, assist in transcription of digital audio-interview

# plugin menu
plugin.h4JmfPlugin.menu=h4Jmf \
	- \
	h4Jmf.choose-file \
	h4Jmf.player-begin \
	h4Jmf.player-start \
	h4Jmf.player-stop \
	h4Jmf.player-rewind \
	h4Jmf.player-forward \
	h4Jmf.insert-time \
	h4Jmf.player-position \
	h4Jmf.player-close
h4Jmf.label=h4Jmf
h4Jmf.title=h4Jmf

h4Jmf.choose-file.label=Choose mp3 file
h4Jmf.player-begin.label=player begin
h4Jmf.player-start.label=player start
h4Jmf.player-stop.label=player stop
h4Jmf.player-rewind.label=player rewind
h4Jmf.player-forward.label=player forward
h4Jmf.insert-time.label=insert time
h4Jmf.player-position.label=player position
h4Jmf.player-close.label=player close

plugin.h4JmfPlugin.option-pane=h4Jmf
options.h4Jmf.code=new h4JmfOptionPane();
options.h4Jmf.label=h4Jmf
#number of seconds to play forward,backward
options.h4Jmf.seconds=7
