    }
}

Type WildcardBounds():
{
    Type type = null;
    String s = "";
}
{
    try {

        (
        "extends" type=ReferenceType() { s="extends"; }
     |
        "super" type=ReferenceType() { s="super"; }
        )
    }
    catch(ParseException pe) {
        error_skipto(SEMICOLON);
    }
    {
        if (type != null)
            type.qualifier = s;
