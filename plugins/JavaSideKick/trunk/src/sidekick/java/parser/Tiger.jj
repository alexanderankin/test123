   LOOKAHEAD( "@" Name() "(" ( <IDENTIFIER> "=" | ")" ))
   list=NormalAnnotation()
 |
   LOOKAHEAD( "@" Name() "(" )
   list=SingleMemberAnnotation()
 |
   list=MarkerAnnotation()
    }
    catch(ParseException pe) {
        // TODO: what to do here?
    }
   {
        return list;   
   }
}

List<TigerNode> NormalAnnotation():
{
    List<TigerNode> list = new ArrayList<TigerNode>();
    NameNode nn = null;
    List<TigerNode> mvp = null;
}
{
    try {
        "@" nn=Name(){nn.setName("@" + nn.getName());} "(" [ mvp=MemberValuePairs() ] ")"
    }
    catch(ParseException pe) {
        error_skipto(RPAREN);   
    }
   
   {
       if (nn != null) {
           list.add(nn);
       }
       if (mvp != null) {
           list.addAll(mvp);
       }
       return list;
   }
}

