|
  LOOKAHEAD(2)
  "<" identifier() ">"
|
  "<"
  // special handling for javacc keyword
  LOOKAHEAD( {getToken(1).kind == IDENTIFIER && getToken(1).image.equals("EOF")} )
  <IDENTIFIER>
  ">"
