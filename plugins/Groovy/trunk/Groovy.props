# ::mode=properties:: #
# Basic Plugin Information {{{
plugin.groovy.GroovyPlugin.activate=startup
plugin.groovy.GroovyPlugin.author=Damien Radtke
plugin.groovy.GroovyPlugin.description=Provides Groovy to jEdit
plugin.groovy.GroovyPlugin.docs=docs/Groovy.html
plugin.groovy.GroovyPlugin.longdescription=description.html
plugin.groovy.GroovyPlugin.name=Groovy
plugin.groovy.GroovyPlugin.usePluginHome=true
plugin.groovy.GroovyPlugin.version=0.1
plugin.groovy.GroovyPlugin.jars=groovy-all-1.7.4.jar
# }}} Basic Plugin Information

# Dependencies {{{
plugin.groovy.GroovyPlugin.depend.0=jedit 04.03.18.00
plugin.groovy.GroovyPlugin.depend.1=jdk 1.5
plugin.groovy.GroovyPlugin.depend.2=optional plugin javadoc.JavadocPlugin 1.1
# }}} Dependencies

