plugin.cppcheck.Plugin.activate=defer

# general plugin information
plugin.cppcheck.Plugin.name=CppCheck
plugin.cppcheck.Plugin.author=Shlomy Reinstein
plugin.cppcheck.Plugin.version=0.1
plugin.cppcheck.Plugin.description=Provides an interface to the CppCheck tool which checks \
  C++ source code for errors that are not ordinarily revealed by compilers.
plugin.cppcheck.Plugin.docs=docs/index.html

plugin.cppcheck.Plugin.depend.0=jdk 1.5
plugin.cppcheck.Plugin.depend.1=jedit 04.04.01.00
plugin.cppcheck.Plugin.depend.2=optional plugin projectviewer.ProjectPlugin 2.9.0.0
plugin.cppcheck.Plugin.depend.3=plugin errorlist.ErrorListPlugin 1.8

# action labels
cppcheck-current-buffer.label=Check current buffer
cppcheck-current-project.label=Check current project
cppcheck-clear.label=Clear cppcheck errors

# menu items
plugin.cppcheck.Plugin.menu=cppcheck-current-buffer \
	cppcheck-current-project \
	cppcheck-clear \
	- \
	cppcheck

# option panes
plugin.cppcheck.Plugin.option-pane=CppCheck
options.CppCheck.label=CppCheck
options.CppCheck.code=new cppcheck.OptionPane();

# file system browser context menu item
plugin.cppcheck.Plugin.browser-menu-item=cppcheck-dir
cppcheck-dir.label=Check directory

# dockables
cppcheck.title=CppCheck
cppcheck.longtitle=CppCheck progress
cppcheck.label=CppCheck

# messages
messages.cppcheck.path=Path to CppCheck:
