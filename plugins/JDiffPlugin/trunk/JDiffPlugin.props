# JDiffPlugin properties
# Author: Andre Kaplan
plugin.jdiff.JDiffPlugin.name=JDiff Plugin
plugin.jdiff.JDiffPlugin.author=Andre Kaplan
plugin.jdiff.JDiffPlugin.version=1.3
plugin.jdiff.JDiffPlugin.docs=JDiffPlugin.html

# Dependencies
plugin.jdiff.JDiffPlugin.depend.0=jdk 1.1
plugin.jdiff.JDiffPlugin.depend.1=jedit 03.02.01.00

# Menu
jdiff-menu=\
    toggle-dual-diff \
    jdiff.show-dual-diff-dialog \
    jdiff.refresh-diff \
    - \
    jdiff.prev-diff \
    jdiff.next-diff \
    - \
    jdiff.toggle-ignore-case \
    jdiff.toggle-trim-whitespace \
    jdiff.toggle-ignore-whitespace \
    - \
    jdiff.diff-normal-output

jdiff-menu.label=JDiff

toggle-dual-diff.label=Dual diff
jdiff.show-dual-diff-dialog.label=Show dual diff dialog
jdiff.refresh-diff.label=Refresh diff
jdiff.prev-diff.label=Go to Previous Difference
jdiff.next-diff.label=Go to Next Difference
jdiff.toggle-ignore-case.label=Ignore case
jdiff.toggle-trim-whitespace.label=Ignore leading and trailing whitespace
jdiff.toggle-ignore-whitespace.label=Ignore all whitespace
jdiff.diff-normal-output.label=Normal Diff Output

jdiff.ignore-case=false
jdiff.trim-whitespace=false
jdiff.ignore-whitespace=false

jdiff.global-virtual-overview=true

jdiff.overview-changed-color=\#FFCC66
jdiff.overview-deleted-color=\#FF6666
jdiff.overview-inserted-color=\#99CC66
jdiff.overview-invalid-color=\#CCCCCC
jdiff.left-cursor-color=\#000000
jdiff.right-cursor-color=\#000000

jdiff.highlight-changed-color=\#FFFF90
jdiff.highlight-deleted-color=\#FF9090
jdiff.highlight-inserted-color=\#D9FF90
jdiff.highlight-invalid-color=\#909090

jdiff.selected-highlight-changed-color=\#FFCC66
jdiff.selected-highlight-deleted-color=\#FF6666
jdiff.selected-highlight-inserted-color=\#99CC66
jdiff.selected-highlight-invalid-color=\#CCCCCC

