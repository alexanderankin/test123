# JDiffPlugin properties
# Author: Andre Kaplan
plugin.jdiff.JDiffPlugin.name=JDiff Plugin
plugin.jdiff.JDiffPlugin.author=Andr√© Kaplan/Mark Wickens, Dale Anson
plugin.jdiff.JDiffPlugin.version=3.1.1
plugin.jdiff.JDiffPlugin.docs=docs/index.html
plugin.jdiff.JDiffPlugin.description=JDiffPlugin is a visual diff and merge utility for jEdit.

# Dependencies
plugin.jdiff.JDiffPlugin.depend.0=jdk 1.5
plugin.jdiff.JDiffPlugin.depend.1=jedit 04.03.15.00
plugin.jdiff.JDiffPlugin.depend.2=plugin CommonControlsPlugin 0.9.5
plugin.jdiff.JDiffPlugin.activate=defer

# Menu
plugin.jdiff.JDiffPlugin.menu=\
    toggle-dual-diff \
    jdiff.show-dual-diff-dialog \
    jdiff.refresh-diff \
    - \
    jdiff.prev-diff \
    jdiff.next-diff \
    jdiff.move-right \
    jdiff.move-left \
    - \
    jdiff.toggle-ignore-case \
    jdiff.toggle-trim-whitespace \
    jdiff.toggle-ignore-amount-whitespace \
    jdiff.toggle-ignore-line-separators \
    jdiff.toggle-ignore-all-whitespace \
    - \
    jdiff.diff-normal-output \
    jdiff.apply-patch

plugin.jdiff.JDiffPlugin.option-group=jdiff.general \
    jdiff.gui \
    jdiff.overview

# titles for DockableWindowManager
jdiff-lines.title=Diff
jdiff-lines.label=Diff

jdiff-menu.label=JDiff

toggle-dual-diff.label=Dual diff
