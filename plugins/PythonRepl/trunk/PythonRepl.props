# ::mode=properties:: #
# Basic Plugin Information {{{
plugin.PythonReplPlugin.activate=startup
plugin.PythonReplPlugin.author=Damien Radtke
plugin.PythonReplPlugin.description=Embeds an interactive Python session into \
	the Console plugin.
plugin.PythonReplPlugin.docs=index.html
plugin.PythonReplPlugin.longdescription=description.html
plugin.PythonReplPlugin.name=PythonRepl
plugin.PythonReplPlugin.usePluginHome=false
plugin.PythonReplPlugin.version=1.0
# }}} Basic Plugin Information

# Dependencies {{{
plugin.PythonPlugin.depend.0=jedit 04.03.18.00
plugin.PythonPlugin.depend.1=jdk 1.5
plugin.PythonPlugin.depend.2=plugin console.ConsolePlugin 4.4.2
# }}} Dependencies

# Menu {{{
plugin.PythonReplPlugin.menu=python-eval-selected python-eval-buffer \
	python-eval-file
python-eval-selected.label=Evaluate Selection in Python shell
python-eval-buffer.label=Evaluate Buffer in Python shell
python-eval-file.label=Evaluate file in Python shell ...
# }}} Menu

# REPL Messages {{{
msg.repl.no-process=No process is currently running.\n
msg.repl.stopped=Stopped.
msg.repl.starting=Starting...\n
msg.repl.error-starting=There was an error starting the process. Check the \
	activity log for details.\n
msg.repl.python.info-message=Interactive Python console.
#}}} REPL Messages
