# ::mode=properties:: #
# Basic Plugin Information {{{
plugin.repl.PythonReplPlugin.activate=defer
plugin.repl.PythonReplPlugin.author=Damien Radtke
plugin.repl.PythonReplPlugin.description=Embeds an interactive Python session into \
	the Console plugin.
#plugin.repl.PythonReplPlugin.docs=index.html
plugin.repl.PythonReplPlugin.longdescription=description.html
plugin.repl.PythonReplPlugin.name=PythonRepl
plugin.repl.PythonReplPlugin.usePluginHome=false
plugin.repl.PythonReplPlugin.version=1.0
# }}} Basic Plugin Information

# Dependencies {{{
plugin.repl.PythonReplPlugin.depend.0=jedit 04.03.99.00
plugin.repl.PythonReplPlugin.depend.1=jdk 1.5
plugin.repl.PythonReplPlugin.depend.2=plugin console.ConsolePlugin 4.4.2
# }}} Dependencies

# Menu {{{
plugin.repl.PythonReplPlugin.menu=python-eval-selected python-eval-buffer \
	python-eval-file
python-eval-selected.label=Evaluate Selection in Python shell
python-eval-buffer.label=Evaluate Buffer in Python shell
python-eval-file.label=Evaluate file in Python shell ...
# }}} Menu

# REPL Messages {{{
msg.repl.no-process=No process is currently running.\n
msg.repl.stopped=Stopped.
msg.repl.starting=Starting...\n
msg.repl.error-starting=There was an error starting the process. Check the \
	activity log for details.\n
msg.repl.python.info-message=Interactive Python console.
#}}} REPL Messages

# Options {{{
plugin.repl.PythonReplPlugin.option-pane=pythonrepl
options.pythonrepl.label=Python Repl
options.pythonrepl.code=new repl.PythonReplOptionPane();
options.pythonrepl.textfield-label=Python executable:
options.pythonrepl.browsebutton-label=Browse...

options.repl.python.exec=python
#}}}

