# Menu
xsearch.label=XSearch
xsearch=xfind xfind-next xfind-prev xfind-refind xfind-first
xfind.label=find
xfind-next.label=find-next
xfind-prev.label=find-prev
xfind-refind.label=find-refind
xfind-first.label=find-first

