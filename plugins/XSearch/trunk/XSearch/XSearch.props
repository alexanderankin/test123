# extended options
search.ext.word-whole=whole word
search.ext.word-prefix=prefix
search.ext.word-suffix=suffix
search.ext.column=column search
search.ext.column.expand-tabs=expand tabs
search.ext.column.left=left col
search.ext.column.right=right col
search.ext.fold=fold search:  
search.ext.fold-inside=inside folds
search.ext.fold-outside=outside folds
search.ext.comment=comment search:  
