<?xml version="1.0"?>

<!DOCTYPE DOCKABLES SYSTEM "dockables.dtd">

<DOCKABLES>
	<DOCKABLE NAME="xml-tree">
		new xml.XmlTree(view,!position.equals(DockableWindowManager.FLOATING));
	</DOCKABLE>
	<DOCKABLE NAME="xml-insert">
		new xml.XmlInsert(view,position.equals(DockableWindowManager.TOP)
			|| position.equals(DockableWindowManager.BOTTOM));
	</DOCKABLE>
</DOCKABLES>
