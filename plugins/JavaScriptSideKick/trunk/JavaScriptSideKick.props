#{{{ general plugin properites
plugin.sidekick.javascript.JavaScriptSideKickPlugin.activate=defer
plugin.sidekick.javascript.JavaScriptSideKickPlugin.name=JavaScriptSideKick
plugin.sidekick.javascript.JavaScriptSideKickPlugin.author=Martin Raspe
plugin.sidekick.javascript.JavaScriptSideKickPlugin.version=0.3
plugin.sidekick.javascript.JavaScriptSideKickPlugin.docs=docs/index.html
# }}}
                
#{{{ dependencies
plugin.sidekick.javascript.JavaScriptSideKickPlugin.depend.0=jdk 1.4.1
plugin.sidekick.javascript.JavaScriptSideKickPlugin.depend.1=jedit 04.02.08.00
plugin.sidekick.javascript.JavaScriptSideKickPlugin.depend.2=plugin errorlist.ErrorListPlugin 1.3.1
plugin.sidekick.javascript.JavaScriptSideKickPlugin.depend.3=plugin sidekick.SideKickPlugin 0.4
# }}}

#{{{ menu
plugin.sidekick.javascript.JavaScriptSideKickPlugin.menu=\
	sidekick-tree \
	javascriptsidekick-toggle-markers-flag \
	javascriptsidekick-undirty-buffer
# }}}

#{{{ action labels
javascriptsidekick-gotodockable.label=Go to structure browser
javascriptsidekick-unload-sidekick-tree.label=Unload sidekick structure browser
javascriptsidekick-toggle-markers-flag.label=Show markers in structure tree
javascriptsidekick-undirty-buffer.label=Clear buffer dirty flag
#}}}

#{{{ registration with SideKick as a mode parser
sidekick.parser.javascript.label=javascript
mode.javascript.sidekick.parser=javascript
# }}}

#{{{ icons
sidekick.parser.javascript.package-icon=package.gif
sidekick.parser.javascript.use-icon=use.gif
sidekick.parser.javascript.sub-icon=sub.gif
sidekick.parser.javascript.package_invalid-icon=package_invalid.gif
sidekick.parser.javascript.use-invalid-icon=use_invalid.gif
sidekick.parser.javascript.sub-invalid-icon=sub_invalid.gif
sidekick.parser.javascript.comment-icon=comment.gif
# }}}

