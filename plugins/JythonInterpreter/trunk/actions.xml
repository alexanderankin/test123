<?xml version="1.0"?>

<!DOCTYPE ACTIONS SYSTEM "actions.dtd">

<ACTIONS>
	<ACTION NAME="jython-about">
		<CODE>
			jython.JythonExecutor.execPlugin("jython.JythonPlugin", "jython",
				"jiabout", "_about", new Object[] {view});
		</CODE>
	</ACTION>
	<ACTION NAME="jython-check-version">
		<CODE>
			jython.JythonExecutor.checkVersion(view);
		</CODE>
	</ACTION>
</ACTIONS>

