<?xml version="1.0"?>

<!DOCTYPE ACTIONS SYSTEM "actions.dtd">

<ACTIONS>
	<ACTION NAME="antfarm">
		<CODE>
    	view.getDockableWindowManager()
    		.toggleDockableWindow("antfarm");
		</CODE>
		<IS_SELECTED>
			return view.getDockableWindowManager()
				.isDockableWindowVisible("antfarm");
		</IS_SELECTED>
	</ACTION>
</ACTIONS>
