### Error parsing

console.error.0.name=VHDL error
console.error.0.match=(.+):.(.+).vhd\((\d+)\):(.+)
console.error.0.filename=$2.vhd
console.error.0.line=$3
console.error.0.message=$4

console.error.1.name=Microsoft Visual C++
console.error.1.filename=$1
console.error.1.line=$2
console.error.1.message=$4
console.error.1.match=([^\\(]*)[\\(](\\d+)[)].\:.(.*).\:.(.*)

console.error.2.name=Microsoft.Net
console.error.2.filename=$1
console.error.2.line=$2
console.error.2.message=$4
console.error.2.match=^(.+)\\((\\d+),(\\d+)\\)\:\\s+(.+)$

console.error.3.name=Perl
console.error.3.match=(.+) at (.+) line (\d+)\.
console.error.3.filename=$2
console.error.3.line=$3
console.error.3.message=$1

console.error.4.name=Jade error
console.error.4.match=(.+?)jade:(.+?):(\\d+?):\\d+?:(.+)
console.error.4.filename=$2
console.error.4.line=$3
console.error.4.message=$4

console.error.5.name=Ant javac error
console.error.5.match=\\s*\\[javac\\]\\s+(.+?):(\\d+):(.+)
console.error.5.filename=$1
console.error.5.line=$2
console.error.5.message=$3

console.error.6.name=Emacs-style error
console.error.6.match=(.+):(\\d+):\\d+:\\d+:\\d+:(.+)
console.error.6.filename=$1
console.error.6.line=$2
console.error.6.message=$3

console.error.7.name=Generic compiler error
console.error.7.match=(.+):(\\d+):(.+)
console.error.7.filename=$1
console.error.7.line=$2
console.error.7.message=$3

