# Plugin properties
plugin.errorlist.ErrorListPlugin.activate=defer

plugin.errorlist.ErrorListPlugin.name=ErrorList
plugin.errorlist.ErrorListPlugin.author=Slava Pestov
plugin.errorlist.ErrorListPlugin.version=1.7
plugin.errorlist.ErrorListPlugin.docs=index.html
plugin.errorlist.ErrorListPlugin.depend.0=jedit 04.03.07.00
plugin.errorlist.ErrorListPlugin.depend.1=jdk 1.5
plugin.errorlist.ErrorListPlugin.description=The ErrorList plugin provides one-click access, and on-the-spot highlighting of errors generated by EditBus-aware packages, such as the Console.
plugin.errorlist.ErrorListPlugin.longdescription=description.html


# Menu
plugin.errorlist.ErrorListPlugin.menu=error-list-show \
	- \
	error-list-previous-error \
	error-list-next-error \
	- \
	error-list-previous-error-file \
	error-list-next-error-file \
	- \
	error-list-clear \
	- \
	error-list-toggle-errors \
	error-list-toggle-warnings

error-list.label=Error List
error-overview.label=Error Overview
error-list-show.label=Error List
error-list-next-error.label=Go to Next Error
error-list-previous-error.label=Go to Previous Error
error-list-next-error-file.label=Go to Next File With Errors
error-list-previous-error-file.label=Go to Previous File With Errors
error-list-clear.label=Clear Error List
error-list-toggle-errors.label=Toggle Display of Errors
error-list-toggle-warnings.label=Toggle Display of Warnings
error-list.showOnError=true

# Option pane
plugin.errorlist.ErrorListPlugin.option-pane=error-list

options.error-list.label=Error List
options.error-list.code=new errorlist.ErrorListOptionPane();
options.error-list.showOnError=Automatically display on error
options.error-list.showErrorOverview=Show error overview
options.error-list.autoCloseOnNoErrors=Auto-close on no errors
options.error-list.autoRefocusTextArea=Auto-refocus on text area
options.error-list.errorColor=Error highlight color:
options.error-list.warningColor=Warning highlight color:
options.error-list.gutterIcons=Show error icons in the gutter
options.error-list.isInclusionFilter=Include filenames matching glob patterns
options.error-list.isExclusionFilter=Exclude filenames matching glob patterns
options.error-list.filenameFilters=Filename filters

# Dockable
error-list.title=Error List
error-list.status.many-error-many-warning={0} errors, {1} warnings
error-list.status.one-error-many-warning={0} error, {1} warnings
error-list.status.many-error-one-warning={0} errors, {1} warning
error-list.status.one-error-one-warning={0} error, {1} warning

# Settings
error-list.showOnError=false
error-list.showErrorOverview=true
error-list.autoCloseOnNoErrors=false
error-list.warningColor=#ffa800
error-list.errorColor=#ff0000
