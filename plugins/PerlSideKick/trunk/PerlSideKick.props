#{{{ general plugin properites
plugin.sidekick.perl.PerlSideKickPlugin.activate=defer
plugin.sidekick.perl.PerlSideKickPlugin.name=PerlSideKick
plugin.sidekick.perl.PerlSideKickPlugin.author=Martin Raspe
plugin.sidekick.perl.PerlSideKickPlugin.version=0.6
plugin.sidekick.perl.PerlSideKickPlugin.docs=docs/index.html
# }}}

#{{{ dependencies
plugin.sidekick.perl.PerlSideKickPlugin.depend.0=jdk 1.5
plugin.sidekick.perl.PerlSideKickPlugin.depend.1=jedit 04.03.03.00
plugin.sidekick.perl.PerlSideKickPlugin.depend.2=plugin errorlist.ErrorListPlugin 1.3.1
plugin.sidekick.perl.PerlSideKickPlugin.depend.3=plugin sidekick.SideKickPlugin 0.5
# }}}

#{{{ menu
plugin.sidekick.perl.PerlSideKickPlugin.menu=sidekick-tree 
# }}}

#{{{ registration with SideKick as a mode parser
sidekick.parser.perl.label=perl
mode.perl.sidekick.parser=perl
# }}}

#{{{ icon names (in the /icons/ subdir)
sidekick.parser.perl.package-icon=perlpackage.gif
sidekick.parser.perl.use-icon=perluse.gif
sidekick.parser.perl.sub-icon=perlsub.gif
sidekick.parser.perl.package_invalid-icon=perlpackage_invalid.gif
sidekick.parser.perl.use-invalid-icon=perluse_invalid.gif
sidekick.parser.perl.sub-invalid-icon=perlsub_invalid.gif
sidekick.parser.perl.comment-icon=perl_comment.gif
# }}}


