	<ACTION NAME="ctags-interface-search-tag-by-substring" NO_REPEAT="TRUE">
		<CODE>
			ctags.CtagsInterfacePlugin.searchTagBySubstring(view);
		</CODE>
	</ACTION>
	<ACTION NAME="ctags-interface-search-tag-by-prefix" NO_REPEAT="TRUE">
		<CODE>
			ctags.CtagsInterfacePlugin.searchTagByPrefix(view);
