<?xml version="1.0"?>

<!DOCTYPE DOCKABLES SYSTEM "dockables.dtd">

<DOCKABLES>
	<DOCKABLE NAME="reference-browser" MOVABLE="TRUE">
		new browser.NewGlobalResultsView(view, "-r -x -a");
	</DOCKABLE>
	<DOCKABLE NAME="definition-browser" MOVABLE="TRUE">
		new browser.NewGlobalResultsView(view, "-x -a");
	</DOCKABLE>
	<DOCKABLE NAME="call-tree-browser" MOVABLE="TRUE">
		new browser.CallTree(view);
	</DOCKABLE>
	<DOCKABLE NAME="pattern-browser" MOVABLE="TRUE">
		new browser.NewGlobalResultsView(view, "-g -x -a");
	</DOCKABLE>
</DOCKABLES>

