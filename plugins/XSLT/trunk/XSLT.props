plugin.xslt.XSLTPlugin.name=XSLT
plugin.xslt.XSLTPlugin.author=Robert McKinnon, Greg Merrill
plugin.xslt.XSLTPlugin.version=0.5
plugin.xslt.XSLTPlugin.docs=index.html
plugin.xslt.XSLTPlugin.jars=xalan.jar
plugin.xslt.XSLTPlugin.depend.0=jdk 1.3
plugin.xslt.XSLTPlugin.depend.1=jedit 04.01.11.00
plugin.xslt.XSLTPlugin.depend.2=plugin xml.XmlPlugin 0.11.1


# XSLT system properties
javax.xml.transform.TransformerFactory=org.apache.xalan.processor.TransformerFactoryImpl
javax.xml.parsers.SAXParserFactory=org.apache.xerces.jaxp.SAXParserFactoryImpl
org.xml.sax.driver=org.apache.xerces.parsers.SAXParser


# Dockables
xslt-processor.label=XSLT Processor
xpath-tool.label=XPath Tool


# Action labels
xslt.transform.label=Transform XML
xslt.settings.load.label=Load XSLT settings
xslt.settings.save.label=Save XSLT settings
xpath.evaluate.label=Evaluate XPath
xslt.indent.label=Indent XML


# Plugin menu
xslt-menu.label=XSLT
xslt-menu=\
  xslt.transform \
  xslt.settings.load \
  xslt.settings.save \
  xslt-processor-toggle \
  - \
  xpath.evaluate \
  xpath-tool-toggle \
  - \
  xslt.indent


# XSLT processor window
xslt-processor.title=XSLT Processor
xslt.message.dock-first=You must dock the XSLT Processor before you can perform this action.
xslt.message.error={0}\nReason: "{1}"\n\nA stacktrace has been written to the Activity Log.

xslt.old-jar.message=The XSLT Plugin is running with an old version of Xalan-Java, \nprobably the 2.2D11 version that is packaged with Sun JDK 1.4.\n\nIf you experience any problems you can try moving to the more recent version \nof Xalan-Java that comes with the XSLT plugin. To use the more recent Xalan-Java: \n\n 1) Go to the directory where the XML and XSLT plugin jars are stored, either\n      your user plugin directory ({0}), or\n      your system plugin directory ({1})\n\n 2) In the plugin directory create a new directory named endorsed, i.e. \n      {2}, or\n      {3}\n\n 3) Move the following jars from the plugin directory into the endorsed directory:\n      xalan.jar, xercesImpl.jar, xmlParserAPIs.jar \n\n 4) When running jEdit, pass java the system property java.endorsed.dirs\n     set to the endorsed directory, for example:\n      java -Djava.endorsed.dirs="{2}" -jar jedit or\n      java -Djava.endorsed.dirs="{3}" -jar jedit\n\nThis information is also available in the XSLT plugin help documentation:\n     Help -> jEdit Help -> Plugins -> XSLT

