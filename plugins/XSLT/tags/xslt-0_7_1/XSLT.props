xslt.factory.class.2=net.sf.saxon.TransformerFactoryImpl
xslt.factory.label.2=XSLT 2.0 - Saxon 9.2 (net.sf.saxon.TransformerFactoryImpl)

# defaults to Xalan
xslt.factory=org.apache.xalan.processor.TransformerFactoryImpl

