<?xml version="1.0"?>
<!DOCTYPE ACTIONS SYSTEM "actions.dtd">
<ACTIONS>
  <ACTION NAME="xslt.transform">
    <CODE>xslt.XsltActions.transformXml(view);</CODE>
  </ACTION>
  <ACTION NAME="xslt.settings.load">
    <CODE>xslt.XsltActions.loadSettings(view);</CODE>
  </ACTION>
  <ACTION NAME="xslt.settings.save">
    <CODE>xslt.XsltActions.saveSettings(view);</CODE>
  </ACTION>
  <ACTION NAME="xpath.evaluate">
    <CODE>xslt.XsltActions.evaluateXpath(view);</CODE>
  </ACTION>
  <ACTION NAME="xslt.indent">
    <CODE>xslt.XsltActions.indentXml(view);</CODE>
  </ACTION>
</ACTIONS>
