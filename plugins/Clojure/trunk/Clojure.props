# ::mode=properties:: #
# Basic Plugin Information {{{
plugin.clojure.ClojurePlugin.activate=startup
plugin.clojure.ClojurePlugin.author=Damien Radtke
plugin.clojure.ClojurePlugin.description=Provides Clojure to jEdit
plugin.clojure.ClojurePlugin.docs=index.html
plugin.clojure.ClojurePlugin.longdescription=description.html
plugin.clojure.ClojurePlugin.name=Clojure
plugin.clojure.ClojurePlugin.usePluginHome=true
plugin.clojure.ClojurePlugin.version=1.2.0
plugin.clojure.ClojurePlugin.jars=clojure-core-1.2.0.jar clojure-contrib-1.1.0.jar
# }}} Basic Plugin Information

# Dependencies {{{
plugin.clojure.ClojurePlugin.depend.0=jedit 04.03.18.00
plugin.clojure.ClojurePlugin.depend.1=jdk 1.5
plugin.clojure.ClojurePlugin.depend.2=optional plugin console.ConsolePlugin 4.4.2
# }}} Dependencies

# Options {{{
plugin.clojure.ClojurePlugin.option-group=clojure-provider
options.clojure-provider.label=Provider
options.clojure-provider.code=new clojure.ClojureProviderOptionPane();
# Possible values:
#	jar - use the jar included with the plugin
#	custom - a user-defined existing installation
options.clojure.install=jar
options.clojure.clojure-path=
options.clojure.clojure-core-jar=clojure-core-1.2.0.jar
options.clojure.clojure-contrib-jar=clojure-contrib-1.1.0.jar

options.clojure.browse-label=Browse...
options.clojure.jar-label=Use included jar
options.clojure.custom-label=Use local jar

options.clojure.path-label=Clojure Jar:
# }}} Options
