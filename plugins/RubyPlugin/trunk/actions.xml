<?xml version="1.0"?>

<!DOCTYPE ACTIONS SYSTEM "actions.dtd">
<ACTIONS>
	<ACTION NAME="ruby.complete-code">
		<CODE>
    	sidekick.SideKickActions.complete(view,sidekick.SideKickActions.COMPLETE_COMMAND);
		</CODE>
	</ACTION>
	<ACTION NAME="ruby.progressive-selection">
		<CODE>
			org.jedit.ruby.RubyActions.progressiveSelection(view);
		</CODE>
	</ACTION>
	<ACTION NAME="ruby.search-documentation">
		<CODE>
			org.jedit.ruby.RubyActions.searchDocumentation(view);
		</CODE>
	</ACTION>
