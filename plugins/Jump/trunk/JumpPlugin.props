#Jump plugin for Jedit4.1
plugin.JumpPlugin.name=Jump!
plugin.JumpPlugin.author=Pavlikus
plugin.JumpPlugin.version=0.1.1

# Dependecies
plugin.JumpPlugin.depend.0=plugin projectviewer.ProjectPlugin 2.0.1

# Documentation
plugin.JumpPlugin.docs=jump.html

# action labels
JumpPlugin.label=Jump!
JumpPlugin.filesjump.label=Jump to file
JumpPlugin.tagsjump.label=Jump to tag
JumpPlugin.workspacejump.label=Search tag
JumpPlugin.historyjump.label=Back (history)
JumpPlugin.reloadtagsonproject.label=Reload tags for project
JumpPlugin.gettagbyinput.label=Type a tag...

# application menu items
JumpPlugin.menu.label=Jump!
JumpPlugin.menu=JumpPlugin.filesjump JumpPlugin.tagsjump JumpPlugin.workspacejump JumpPlugin.historyjump JumpPlugin.gettagbyinput JumpPlugin.reloadtagsonproject

# options
options.JumpPlugin.label=Jump!
options.JumpPlugin.enable.label=Enable Jump!
options.JumpPlugin.parse_on_save.label=Parse buffer on save
options.JumpPlugin.ctags.def.path.label=Path to ctags executable: (C:\\catgs55\\ctags.exe)
options.JumpPlugin.ctags.def.path=C:/Apps/ctags/ctags.exe


# messages
JumpPlugin.no_project.title=Jump! - Error
JumpPlugin.no_project.message=Error during starting Jump!\nMake sure path to ctags correct\nand(or) any project is active.

JumpPlugin.input_tag.title=Jump! - Jump to tag
JumpPlugin.input_tag.message=Enter tag to jump to:

JumpPlugin.ctags.path.incorrect.title=Jump! - Starting ctags error
JumpPlugin.ctags.path.incorrect.message=Failed to start ctags application.\nCheck Jump! options for correct path.

JumpPlugin.ctags.test.complete.title=Jump! - Test ctags
JumpPlugin.ctags.test.complete.message=Good news! Test complete.

JumpPlugin.ctags.test.fail.title=Jump! - Test ctags
JumpPlugin.ctags.test.fail.message=Test failed. Exception during starting ctags.

JumpPlugin.enable.title=Jump! is disabled now.
JumpPlugin.enable.message=Jump! is disabled now. To enable it, see Options-Jump!

JumpPlugin.newproject.title=Jump! Generate tags on project
JumpPlugin.newproject.message=Jump! Now you need to create tag file on project.\nBe patient, it may take up some time.\nIt depend on project size.
