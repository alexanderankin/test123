# Plugin properties
plugin.infoviewer.InfoViewerPlugin.name=Info Viewer
plugin.infoviewer.InfoViewerPlugin.activate=defer

plugin.infoviewer.InfoViewerPlugin.author=Dirk Moebius
plugin.infoviewer.InfoViewerPlugin.version=1.5
plugin.infoviewer.InfoViewerPlugin.docs=index.html

plugin.infoviewer.InfoViewerPlugin.depend.0=jedit 04.03.03.00

# Two option panes
plugin.infoviewer.InfoViewerPlugin.option-group=chooseBrowser internalBrowser

# first optionpane
options.chooseBrowser.label=Choose Browser
options.chooseBrowser.code=new infoviewer.InfoViewerOptionPane();

# second optionpane 
options.internalBrowser.label=Internal Browser
options.internalBrowser.code=new infoviewer.InfoViewerOptionPane2();

# plugin menu
plugin.infoviewer.InfoViewerPlugin.menu = infoviewer.open - infoviewer.open-buffer infoviewer.open-sel - infoviewer - infoviewer.increase-viewer-fontsize infoviewer.decrease-viewer-fontsize

# Plugin menu properties 
infoviewer.label=Internal InfoViewer
