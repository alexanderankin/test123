<!DOCTYPE ACTIONS SYSTEM "actions.dtd">

<ACTIONS>

	<ACTION NAME="infonode-start" NO_REPEAT="TRUE">
		<CODE>
			infonode.Plugin.doStart(view);
		</CODE>
	</ACTION>
	<ACTION NAME="infonode-load" NO_REPEAT="TRUE">
		<CODE>
			infonode.Plugin.load(view);
		</CODE>
	</ACTION>
	<ACTION NAME="infonode-save" NO_REPEAT="TRUE">
		<CODE>
			infonode.Plugin.save(view);
		</CODE>
	</ACTION>
	<ACTION NAME="infonode-select-theme" NO_REPEAT="TRUE">
		<CODE>
			infonode.Plugin.selectTheme(view);
		</CODE>
	</ACTION>

</ACTIONS>

