<!DOCTYPE DOCKABLES SYSTEM "dockables.dtd">

<DOCKABLES>
	<DOCKABLE NAME="debugger-show-locals">
		perl.core.Debugger.getInstance().showLocals(view);
	</DOCKABLE>
	<DOCKABLE NAME="debugger-watches">
		perl.core.Debugger.getInstance().showWatches(view);
	</DOCKABLE>
	<DOCKABLE NAME="debugger-show-variables">
		perl.core.Debugger.getInstance().showVariables(view);
	</DOCKABLE>
	<DOCKABLE NAME="debugger-show-stack-trace">
		perl.core.Debugger.getInstance().showStackTrace(view);
	</DOCKABLE>
	<DOCKABLE NAME="debugger-breakpoints">
		perl.core.Debugger.getInstance().showBreakpoints(view);
	</DOCKABLE>
	<DOCKABLE NAME="debugger-perl-breakpoints">
		perl.core.Debugger.getInstance().showGdbBreakpoints(view);
	</DOCKABLE>
	<DOCKABLE NAME="debugger-panel">
		perl.core.Debugger.getInstance().showControlPanel(view);
	</DOCKABLE>
</DOCKABLES>

