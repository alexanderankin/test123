<!DOCTYPE DOCKABLES SYSTEM "dockables.dtd">

<DOCKABLES>
	<DOCKABLE NAME="perl-dbg-locals" MOVABLE="TRUE">
		new perl.dockables.Locals(view);
	</DOCKABLE>
	<DOCKABLE NAME="perl-dbg-watches" MOVABLE="TRUE">
		new perl.dockables.Watches(view);
	</DOCKABLE>
	<DOCKABLE NAME="perl-dbg-stack" MOVABLE="TRUE">
		new perl.dockables.Stack(view);
	</DOCKABLE>
	<DOCKABLE NAME="perl-dbg-breakpoints" MOVABLE="TRUE">
		new perl.dockables.Breakpoints(view);
	</DOCKABLE>
	<DOCKABLE NAME="perl-dbg-panel">
		new perl.dockables.ControlPanel(view);
	</DOCKABLE>
</DOCKABLES>

