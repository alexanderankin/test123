plugin.browser.ClassBrowserPlugin.activate=defer

# general plugin information
plugin.browser.ClassBrowserPlugin.name=Class Browser
plugin.browser.ClassBrowserPlugin.author=Shlomy Reinstein
plugin.browser.ClassBrowserPlugin.version=1.1
plugin.browser.ClassBrowserPlugin.description=\
	The ClassBrowser plugin provides a dockable window presenting the hierarchy of a selected class, along with its members (including inherited members, if requested). You can jump to each element (class/member) in the hierarchy by double-clicking it. The hierarchy is generated using information from Ctags tag files.

plugin.browser.ClassBrowserPlugin.depend.0=jdk 1.4
plugin.browser.ClassBrowserPlugin.depend.1=jedit 04.03.03.00
plugin.browser.ClassBrowserPlugin.depend.2=plugin tags.TagsPlugin 2.0.2

# action labels
class-browser-show-hierarchy.label=Show Class Hierarchy ..
class-browser-show-selected-hierarchy.label=Show Class Hierarchy

# menu items
plugin.browser.ClassBrowserPlugin.menu=class-browser-show-hierarchy \
  class-browser-show-selected-hierarchy

# dockables
class-browser-class-hierarchy.title=ClassBrowser
class-browser-class-hierarchy.longtitle=Class Hierarchy Browser
class-browser-class-hierarchy.label=Class Hierarchy Browser
# docs
plugin.browser.ClassBrowserPlugin.docs=index.html

# icons
options.ClassBrowser.icons.class=class.png
options.ClassBrowser.icons.field=variable.png
options.ClassBrowser.icons.function=function.png
options.ClassBrowser.icons.macro=macro.png
options.ClassBrowser.icons.member=variable.png
options.ClassBrowser.icons.method=function.png
options.ClassBrowser.icons.package=package.png
options.ClassBrowser.icons.procedure=function.png
options.ClassBrowser.icons.prototype=prototype.png
options.ClassBrowser.icons.struct=struct.png
options.ClassBrowser.icons.typedef=typedef.png
options.ClassBrowser.icons.union=union.png
options.ClassBrowser.icons.variable=variable.png
options.ClassBrowser.icons.enum=enum.png
options.ClassBrowser.icons.enumerator=enumerator.png

