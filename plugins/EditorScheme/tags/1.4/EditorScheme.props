plugin.editorscheme.EditorSchemePlugin.activate=defer

plugin.editorscheme.EditorSchemePlugin.name=Editor Scheme
plugin.editorscheme.EditorSchemePlugin.author=Ollie Rutherfurd
plugin.editorscheme.EditorSchemePlugin.version=1.4
plugin.editorscheme.EditorSchemePlugin.description=EditorScheme lets one easily switch between predefined color and style combinations for jEdit's TextArea and a number of jEdit's Plugins
plugin.editorscheme.EditorSchemePlugin.docs=index.html

plugin.editorscheme.EditorSchemePlugin.depend.0=jedit 04.03.99.00
plugin.editorscheme.EditorSchemePlugin.depend.1=jdk 1.5


