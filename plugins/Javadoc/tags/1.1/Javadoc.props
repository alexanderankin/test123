plugin.javadoc.JavadocPlugin.activate=startup
plugin.javadoc.JavadocPlugin.name=Javadoc
plugin.javadoc.JavadocPlugin.author=Damien Radtke
plugin.javadoc.JavadocPlugin.version=1.1
