<?xml version="1.0"?>

<!DOCTYPE DOCKABLES SYSTEM "dockables.dtd">

<DOCKABLES>
    <DOCKABLE NAME="navigation-dock">
        uk.co.antroy.latextools.LaTeXParser.getControls();
    </DOCKABLE>
    <DOCKABLE NAME="bibliography-dock">
        new uk.co.antroy.latextools.BibTeXPanel(view, buffer);
    </DOCKABLE>
    <DOCKABLE NAME="label-dock">
			  new uk.co.antroy.latextools.ReferencePanel(view, buffer);
    </DOCKABLE>
    <DOCKABLE NAME="image-viewer-dock">
        viewer = uk.co.antroy.latextools.ImageViewer.getInstance();
        viewer.showImage();
    </DOCKABLE>
    <DOCKABLE NAME="project-viewer-dock">
        new uk.co.antroy.latextools.ProjectViewerPanel(view, buffer);
    </DOCKABLE>
</DOCKABLES>

